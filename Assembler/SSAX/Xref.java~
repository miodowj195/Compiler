import java.util.*;

public class Xref {

    public int defined;
    public Stack<Integer> xrefList;

    public Xref() {
	defined = 0;
	xrefList = new Stack<Integer>();
    }

    // add n to the cross-reference list
    public void addXref(int n) {
	if (xrefList.size() == 0 || xrefList.peek() != n)
	    xrefList.push(n);
    }

    // set defined to be n and add n to the cross-reference list
    public void setXref(int n) {
	defined = n;
	addXref(n);    // do this too
    }

    public Stack<Integer> getXrefList() {
	return xrefList;
    }

}
